REACT_APP_GOOGLE_TRANSLATE_API_KEY=your_google_translate_api_key_here

# LibreTranslate API Key (Optional for public instance)
# Get your key from: https://libretranslate.com/
REACT_APP_LIBRETRANSLATE_API_KEY=your_libretranslate_api_key_here

